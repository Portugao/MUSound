{
    "name": "mu/sound-module",
    "version": "1.2.0",
    "description": "MUSoundModule module generated by ModuleStudio 1.0.1.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\SoundModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.5.0 <3.0",
            "class": "MU\\SoundModule\\MUSoundModule",
            "displayname": "Sound",
            "url": "sound",
            "capabilities": {
                "user": {"route": "musoundmodule_collection_index"},
                "admin": {"route": "musoundmodule_collection_adminindex"},
                "categorizable": {
                    "entities": [
                        "MU\\SoundModule\\Entity\\AlbumEntity"
                    ]
                }
            },
            "securityschema": {
                "MUSoundModule::": "::",
                "MUSoundModule:ItemListBlock:": "Block title::",
                "MUSoundModule:ModerationBlock:": "Block title::",
                "MUSoundModule:Album:": "Album ID::",
                "MUSoundModule:Collection:Album": "Collection ID:Album ID:",
                "MUSoundModule:Track:": "Track ID::",
                "MUSoundModule:Album:Track": "Album ID:Track ID:",
                "MUSoundModule:Collection:": "Collection ID::",
                "MUSoundModule::Ajax": "::"
            }
        }
    }
}
